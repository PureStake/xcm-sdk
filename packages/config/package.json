{
  "name": "@moonbeam-network/xcm-config",
  "version": "0.1.1",
  "description": "All necessary configuration to transfer assets from Moonbeam, Moonriver, Moonbase to other parachains and back",
  "repository": {
    "directory": "packages/config",
    "type": "git",
    "url": "git+https://github.com/PureStake/xcm-sdk.git"
  },
  "keywords": [
    "moonbeam",
    "moonriver",
    "xcm"
  ],
  "author": "PureStake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PureStake/xcm-sdk/issues"
  },
  "homepage": "https://docs.moonbeam.network/builders/xcm/xcm-sdk/xcm-sdk/",
  "files": [
    "build"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "import": {
        "types": "./build/mjs/src/index.d.ts",
        "default": "./build/mjs/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "types": "./build/cjs/src/index.d.ts",
  "main": "./build/cjs/src/index.js",
  "dependencies": {
    "@moonbeam-network/api-augment": "^0.1801.0",
    "@polkadot/types": "^9.6.1",
    "@polkadot/util": "^10.1.11",
    "@polkadot/util-crypto": "^10.1.11"
  }
}
