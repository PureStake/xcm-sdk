{
  "name": "@moonbeam-network/xcm-config",
  "version": "0.4.3",
  "description": "All necessary configuration to transfer assets from Moonbeam, Moonriver, Moonbase to other parachains and back",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "link": "npm ln",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest /src",
    "test:watch": "npm test -- --watch",
    "test:update": "npm test -- -u",
    "test:ci": "npm test -- --ci",
    "prepack": "prettier --write build/index.d.ts"
  },
  "repository": {
    "directory": "packages/config",
    "type": "git",
    "url": "git+https://github.com/PureStake/xcm-sdk.git"
  },
  "keywords": [
    "moonbeam",
    "moonriver",
    "xcm"
  ],
  "author": "PureStake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PureStake/xcm-sdk/issues"
  },
  "homepage": "https://docs.moonbeam.network/builders/xcm/xcm-sdk/xcm-sdk/",
  "files": [
    "build"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.ts",
        "default": "./build/index.cjs"
      }
    }
  },
  "types": "./build/index.d.ts",
  "main": "./build/index.cjs",
  "dependencies": {
    "@moonbeam-network/xcm-builder": "*",
    "@moonbeam-network/xcm-types": "*"
  },
  "peerDependencies": {
    "@polkadot/api": "10.x",
    "@polkadot/api-augment": "10.x",
    "@polkadot/types": "10.x",
    "@polkadot/util": "11.x",
    "@polkadot/util-crypto": "11.x"
  }
}
