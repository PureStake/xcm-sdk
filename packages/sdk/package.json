{
  "name": "@moonbeam-network/xcm-sdk",
  "version": "0.0.25",
  "description": "The Moonbeam XCM SDK enables developers to easily deposit and withdraw assets to Moonbeam/Moonriver from the relay chain and other parachains in the Polkadot/Kusama ecosystem",
  "author": "PureStake",
  "license": "MIT",
  "repository": {
    "directory": "packages/sdk",
    "type": "git",
    "url": "git+https://github.com/PureStake/xcm-sdk.git"
  },
  "keywords": [
    "moonbeam",
    "moonriver",
    "xcm"
  ],
  "bugs": {
    "url": "https://github.com/PureStake/xcm-sdk/issues"
  },
  "homepage": "https://github.com/PureStake/xcm-sdk#readme",
  "files": [
    "build"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "import": {
        "types": "./build/mjs/src/index.d.ts",
        "default": "./build/mjs/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "types": "./build/cjs/src/index.d.ts",
  "main": "./build/cjs/src/index.js",
  "dependencies": {
    "@moonbeam-network/api-augment": "^0.1801.0",
    "@moonbeam-network/xcm-config": "*",
    "@polkadot/api": "^9.4.3",
    "@polkadot/types": "^9.4.3",
    "@polkadot/util": "^10.1.9",
    "ethers": "^5.7.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186"
  }
}
